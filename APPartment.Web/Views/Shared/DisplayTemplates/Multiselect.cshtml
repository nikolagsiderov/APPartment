@model List<long>

@{ 
    List<SelectListItem> selectList = ViewData[ViewData.TemplateInfo.HtmlFieldPrefix] as List<SelectListItem>;
}

<div class="form-group">
    <select multiple class="form-control form-control-solid" disabled="disabled" name="@ViewData.TemplateInfo.HtmlFieldPrefix">
        @if (selectList.Any(x => x.Selected))
        {
            foreach (var option in selectList.Where(x => x.Selected))
            {
                <option selected="@option.Selected" value="@option.Value">@option.Text</option>
            }
        }
        else
        {
            <option>None selected</option>
        }
    </select>
</div>