@model APPartment.Infrastructure.UI.Common.ViewModels.Base.PostViewModel

<ul class="nav nav-tabs" id="myTab" role="tablist">
    <li class="nav-item">
        <a class="nav-link active" id="comments-tab" data-toggle="tab" href="#commentsTab" role="tab" aria-controls="commentsTab" aria-selected="true"><i class="far fa-comments"></i> Comments</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="images-tab" data-toggle="tab" href="#imagesTab" role="tab" aria-controls="imagesTab" aria-selected="false"><i class="far fa-images"></i> Images</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="participants-tab" data-toggle="tab" href="#participantsTab" role="tab" aria-controls="participantsTab" aria-selected="false"><i class="fas fa-user-friends"></i> Participants</a>
    </li>
</ul>
<div class="tab-content" id="myTabContent">
    <div class="tab-pane fade show active" id="commentsTab" role="tabpanel" aria-labelledby="comments-tab">
        <br />
        <partial name="_Comments", model="Model" />
    </div>
    <div class="tab-pane fade" id="imagesTab" role="tabpanel" aria-labelledby="images-tab">
        <br />
        <partial name="_Images" , model="Model" />
    </div>
    <div class="tab-pane fade" id="participantsTab" role="tabpanel" aria-labelledby="participants-tab">
        <br />
        <partial name="_Participants" , model="Model" />
    </div>
</div>