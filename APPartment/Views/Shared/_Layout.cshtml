<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>

    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>

    <link rel="icon" href="~/images/APPartment_logo_official_white_favicon.ico">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <link href="~/css/styles.css" rel="stylesheet">
    <link href="~/css/dropzone.min.css" rel="stylesheet" />
    <link href="~/css/layout.custom.css" rel="stylesheet" />

    <link href="~/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/fullcalendar.css">
    <link href="~/css/jquery.fancybox.min.css" rel="stylesheet" />
    <script src="~/js/feather.min.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/css/nav.clock.css" />

    @RenderSection("Scripts", required: false)
</head>
<body id="nav-fixed">
    <partial name="_Navigation" />
    <div id="layoutSidenav">
        <partial name="_Menu" />
        <div id="layoutSidenav_content">
            <main>
                <div class="page-header pb-10 page-header-dark bg-gradient-primary-to-secondary">
                    <div class="container-fluid">
                        <partial name="_NavClock" />
                        <div class="page-header-content">
                            <breadcrumb></breadcrumb>
                        </div>
                    </div>
                </div>
                <div class="container-fluid mt-n15">
                    @RenderBody()
                </div>
            </main>
            <partial name="_Footer" />
        </div>
    </div>

    <partial name="_SetHomeStatus" />

    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <script src="~/js/scripts.js"></script>

    <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>

    <script src="~/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="~/vendor/datatables/dataTables.bootstrap4.min.js"></script>
    <script src="~/js/jquery.fancybox.min.js"></script>
    <script src="~/js/moment.js"></script>
    <script src="~/js/fullcalendar.js"></script>
    <script src="~/js/dropzone.min.js"></script>

    <script src="~/js/layout.custom.js"></script>
    <script src="~/js/nav.clock.js"></script>
</body>
</html>