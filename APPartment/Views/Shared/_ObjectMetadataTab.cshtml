@model APPartment.Data.Server.Models.Base.BaseObject

<ul class="nav nav-tabs" id="myTab" role="tablist">
    <li class="nav-item">
        <a class="nav-link active" id="comments-tab" data-toggle="tab" href="#commentsTab" role="tab" aria-controls="commentsTab" aria-selected="true"><i class="far fa-comments"></i> Comments</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="images-tab" data-toggle="tab" href="#imagesTab" role="tab" aria-controls="imagesTab" aria-selected="false"><i class="far fa-images"></i> Images</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="history-tab" data-toggle="tab" href="#historyTab" role="tab" aria-controls="historyTab" aria-selected="false"><i class="fas fa-history"></i> History</a>
    </li>
</ul>
<div class="tab-content" id="myTabContent">
    <div class="tab-pane fade show active" id="commentsTab" role="tabpanel" aria-labelledby="comments-tab">
        <br />
        @Html.Partial("_Comments", Model)
    </div>
    <div class="tab-pane fade" id="imagesTab" role="tabpanel" aria-labelledby="images-tab">
        <br />
        @Html.Partial("_Images", Model)
    </div>
    <div class="tab-pane fade" id="historyTab" role="tabpanel" aria-labelledby="history-tab">
        <br />
        @Html.Partial("_History", Model)
    </div>
</div>